@article{Jenssen2007796,
title = "Information cut for clustering using a gradient descent approach ",
journal = "Pattern Recognition ",
volume = "40",
number = "3",
pages = "796 - 806",
year = "2007",
note = "",
issn = "0031-3203",
doi = "http://dx.doi.org/10.1016/j.patcog.2006.06.028",
url = "http://www.sciencedirect.com/science/article/pii/S0031320306003037",
author = "Robert Jenssen and Deniz Erdogmus and Kenneth E. Hild II and Jose C. Principe and Torbjørn Eltoft",
keywords = "Graph theoretic cut",
keywords = "Information theory",
keywords = "Parzen window density estimation",
keywords = "Clustering",
keywords = "Gradient descent optimization",
keywords = "Annealing ",
abstract = "We introduce a new graph cut for clustering which we call the Information Cut. It is derived using Parzen windowing to estimate an information theoretic distance measure between probability density functions. We propose to optimize the Information Cut using a gradient descent-based approach. Our algorithm has several advantages compared to many other graph-based methods in terms of determining an appropriate affinity measure, computational complexity, memory requirements and coping with different data scales. We show that our method may produce clustering and image segmentation results comparable or better than the state-of-the art graph-based methods. "
}

